   "source": [
    "car_brands = autos['brand'].value_counts().index\n",
    "car_brands_dict = {}\n",
    "for brands in car_brands:\n",
    "    selected_rows = autos[autos[\"brand\"] == brands]\n",
    "    mean = selected_rows['price'].mean()\n",
    "    \n",
    "    car_brands_dict[brands] = mean"
   ]
